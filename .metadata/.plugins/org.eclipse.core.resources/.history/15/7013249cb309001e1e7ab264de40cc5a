package LinkedList;

public class CreateLinkedList {
	
	public static class Node{
		int data ; 
		Node Next ; 
		
		Node(int data){
			
			this.data= data;
			
		}
		
	}
	
	public static void main (String args[]) {
		
		Node node = new Node(99);
		Node node1 = new Node(909);
		Node node2 = new Node(990);
		
		node.Next = node1;
		
		
		
		
		System.out.println(node.Next.data);
		
		
	}

}
