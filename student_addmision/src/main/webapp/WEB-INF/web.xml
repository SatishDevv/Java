<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<context-param>
		<param-name>schoole</param-name>
		<param-value>FC College</param-value>
	</context-param>
	<servlet>
		<servlet-name>signup</servlet-name>
		<servlet-class>student_addmision.SignupRequest</servlet-class>
		<init-param>
			<param-name>onetimepay</param-name>
			<param-value>150000</param-value>
		</init-param>
		<init-param>
			<param-name>install</param-name>
			<param-value>200000</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
</web-app>
